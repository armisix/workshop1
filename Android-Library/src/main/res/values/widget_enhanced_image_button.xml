<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EnhancedImageButton">
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="android:src" />
        <attr name="eib_image_drawable" format="reference" />
        <attr name="eib_image_drawableAlt" format="reference" />
        <attr name="eib_image_width" format="dimension" />
        <attr name="eib_image_height" format="dimension" />
        <attr name="eib_selectableArea" format="enum">
            <enum name="none" value="-1" />
            <enum name="parent" value="0" />
            <enum name="child" value="1" />
        </attr>
        <attr name="eib_colorNormal" format="color|reference" />
        <attr name="eib_colorDisabled" format="color|reference" />
        <attr name="eib_colorActivated" format="color|reference" />
        <attr name="eib_tintMode" format="enum">
            <enum name="normal" value="0" />
            <enum name="disabled" value="1" />
            <enum name="singleColor" value="2" />
        </attr>
        <attr name="eib_colorNormalAlt" format="color|reference" />
        <attr name="eib_colorDisabledAlt" format="color|reference" />
        <attr name="eib_colorActivatedAlt" format="color|reference" />
        <attr name="eib_tintModeAlt" format="enum">
            <enum name="normal" value="0" />
            <enum name="disabled" value="1" />
            <enum name="singleColor" value="2" />
        </attr>
    </declare-styleable>
</resources>